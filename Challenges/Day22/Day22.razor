@page "/Day22"
@inject DataFetcher dataFetcher

<PageTitle>AoC 2023 - Day 22</PageTitle>

<h3>Day 22</h3>

<pre>
    @inputData
</pre>

<button @onclick=@LoadData>Load The Data</button>


@code {
    private readonly int dayNumber = 22;
    string inputData = string.Empty;

    private async Task LoadData()
    {
        inputData = await dataFetcher.FetchDataAsync(dayNumber);
    }
}
